<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<routeInfoList>
    <routeInfo>
        <serviceID>
            <business>business</business>
            <service>service</service>
        </serviceID>
        <nodes>
            <node>
                <type>DYNAMIC</type>
                <host>127.0.0.1</host>
                <port>50030</port>
                <weight>100</weight>
            </node>
        </nodes>
        <rules>
            <rule>
                <type>HOST</type>
                <srcOp>EQUAL</srcOp>
                <srcValue>192.168.1.*</srcValue>
                <destination>10.136.4.*</destination>
            </rule>
            <rule>
                <type>METHOD_ARGS</type>
                <srcProp>QQ</srcProp>
                <srcOp>BETWEEN</srcOp>
                <srcValue>1000-2000</srcValue>
                <destination>10.136.4.*</destination>
            </rule>
        </rules>
        <strategies>
            <entry>
                <key>FAULT_TOLERANCE</key>
                <value>
                    <type>FAULT_TOLERANCE</type>
                    <option>FAILOVER</option>
                    <config>
                        <entry>
                            <key>failover.retries</key>
                            <value>2</value>
                        </entry>
                    </config>
                </value>
            </entry>
            <entry>
                <key>ARBITER</key>
                <value>
                    <type>ARBITER</type>
                    <config>
                        <entry>
                            <key>arbite.blacklist.effective.times</key>
                            <value>2</value>
                        </entry>
                        <entry>
                            <key>arbite.blacklist.effective.period</key>
                            <value>300</value>
                        </entry>
                        <entry>
                            <key>arbite.blacklist.expire.period</key>
                            <value>60</value>
                        </entry>
                    </config>
                </value>
            </entry>
            <entry>
                <key>LOAD_BALANCE</key>
                <value>
                    <type>LOAD_BALANCE</type>
                    <option>ROUND_ROBIN</option>
                    <config/>
                </value>
            </entry>
        </strategies>
    </routeInfo>
</routeInfoList>